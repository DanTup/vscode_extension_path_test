{
    "name": "path-test",
    "displayName": "path-test",
    "description": "",
    "version": "0.0.1",
    "publisher": "DanTup",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "echo $PATH && npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.5.1",
        "vscode": "^1.1.34",
        "@types/node": "^12.0.4",
        "@types/mocha": "^5.2.7"
    }
}
